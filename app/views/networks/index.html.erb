<h2 id="network">Mes Foundates</h2>
<div class="containerperso">
  <div class="disposition">
    <% @contacts.each do |contact| %>
      <%= link_to user_path(contact) do %>
        <div class="cards" style="background-image: url('<%= cl_image_path contact.photo.key %>');">
          <div class="networkname">
            <%= contact.last_name %>
            <%= contact.first_name %>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>

